{
  "name": "transportation-ops-platform",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.2",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@tanstack/react-query": "^5.28.5",
    "axios": "^1.7.9",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.5",
    "mapbox-gl": "^3.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-map-gl": "^7.1.6",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.7",
    "socket.io-client": "^4.7.5",
    "zustand": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@testing-library/react-hooks": "@testing-library/react-hooks/lib/dom/pure",
      "^@testing-library/react": "@testing-library/react",
      "^@testing-library/user-event": "@testing-library/user-event",
      "^@testing-library/jest-dom": "@testing-library/jest-dom"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs|ts|tsx)$": "babel-jest",
      "^.+\\.(css|scss|sass|less)$": "jest-transform-css",
      "^.+\\.(jpg|jpeg|png|gif|webp|svg|eot|otf|ttf|woff|woff2)$": "<rootDir>/fileTransformer.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(?:@mui|@babel/runtime|@emotion|@testing-library|react-router-dom|@testing-library/user-event|@testing-library/dom|@testing-library/jest-dom)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/cypress/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/setupTests.js",
      "src/reportWebVitals.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-css": "^6.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.0",
    "vite-plugin-pwa": "^0.17.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
